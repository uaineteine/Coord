<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <Authors>Daniel Stamer-Squair</Authors>
    <Company>Daniel Stamer-Squair</Company>
    <SignAssembly>false</SignAssembly>
    <Copyright>Copyright Â© 2021 Daniel Stamer-Squair</Copyright>
    <RootNamespace>Uaine.Coord</RootNamespace>
    <AssemblyName>Uaine.Coord</AssemblyName>
    <Version>1.3.2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>A C# .NET standard project for co-ordinate classes with additional functions</Description>
    <PackageIconUrl></PackageIconUrl>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/uaineteine/Coord</PackageProjectUrl>
    <RepositoryUrl>https://github.com/uaineteine/Coord</RepositoryUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIcon>icon.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\icon\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
